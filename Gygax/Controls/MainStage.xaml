<UserControl x:Class="GygaxVisu.Controls.MainStage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GygaxVisu.Controls"
             xmlns:hx="http://helix-toolkit.org/wpf/SharpDX"
             xmlns:sdx="clr-namespace:SharpDX;assembly=SharpDX"
             xmlns:gygaxCore="clr-namespace:GygaxCore;assembly=GygaxCore"
             xmlns:devices="clr-namespace:GygaxCore.Devices;assembly=GygaxCore"
             mc:Ignorable="d" >
    <Grid Name="MainGrid" DataContext="{Binding ElementName=StreamList, Path=SelectedItem}">
        <ContentControl Name="SelectedItemControl" />
    </Grid>
</UserControl>
